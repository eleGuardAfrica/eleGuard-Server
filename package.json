{
  "name": "eleguard-server",
  "version": "1.0.0",
  "description": "A node.js server for eleguard project",
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "start:dev": "nodemon  --watch 'src/**/*.ts' --exec ts-node-dev --respawn --transpile-only ./src/server.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "lint": "eslint 'src/**/*.ts'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "express",
    "typescript",
    "api",
    "contacts"
  ],
  "author": "Your Name",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "axios": "^1.10.0",
    "btoa": "^1.2.1",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^24.0.3",
    "nodemon": "^3.1.10",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
